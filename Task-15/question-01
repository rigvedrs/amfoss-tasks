#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main(){
    
    int t;
    cin>>t;
    for(int c=0;c<t;c++)
    {
    int n;
    cin>>n;
    int sum = 0;
    
    for(int i=1;i<n;i++)
    {
        
        if((i%3==0)||(i%5==0))
        sum = sum+i;
        
    }
    
    cout<<sum<<endl;
    }
    return 0;
}
